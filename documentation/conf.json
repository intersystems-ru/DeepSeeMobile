{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["www/js"],
        "exclude": ["www/js/lib","www/js/r.js","www/js/main-build.js"],
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "template",
        "encoding": "utf8", 
        "destination": "documentation/", 
        "private": true
    }
}